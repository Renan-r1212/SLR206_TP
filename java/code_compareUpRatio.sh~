mkdir genResult

for FILE in 1 2 3 4
do
    mkdir genResult/update_compare_$FILE/ 
    for ALG in CoarseGrainedListBasedSet HandsOverHandsSet
    do
        for THREADS in 1 4 8 12
        do
            for UPDATE_RATIO in 10 100
            do
                for LIST_SIZE in 1000 2000 5000 8000 
                do
                
                    LIST_RANGE=$((LIST_SIZE * 2))
                    java -cp bin contention.benchmark.Test -b linkedlists.lockbased.$ALG -W 0 -d 2000 -t $THREADS -u $UPDATE_RATIO -i $LIST_SIZE -r $LIST_RANGE > genResult/update_compare_$FILE/$LIST_SIZE"_"$UPDATE_RATIO"_"$ALG"_"$THREADS"_"$LIST_RANGE.txt
    
                done
            done
        done
    done
done
